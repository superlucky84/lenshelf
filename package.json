{
  "type": "module",
  "name": "lenshelf",
  "version": "0.0.1",
  "description": "X UI Library Universal State Management Library",
  "packageManager": "pnpm@7.6.0",
  "main": "./dist/lenshelf.umd.js",
  "module": "./dist/lenshelf.mjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:core": "vitest run --reporter verbose",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "rm -rf dist",
    "clean:module": "rm -rf node_modules"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/lenshelf.mjs",
      "require": "./dist/lenshelf.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "author": "superlucky84",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/lenshelf.git"
    
  },
  "homepage": "https://github.com/superlucky84/lenshelf",
  "license": "MIT",
  "keywords": [
    "lenshelf"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^9.20.3",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "terser": "^5.18.1",
    "typescript": "^4.9.5",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^1.6.0"
  },
  "volta": {
    "node": "20.3.0"
  }
}
