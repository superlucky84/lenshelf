  "description": "X UI Library Universal State Management Library",
  "packageManager": "pnpm@7.6.0",
  "main": "./dist/lenshelf.umd.js",
  "module": "./dist/lenshelf.mjs",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test:all": "vitest run --reporter verbose",
    "test": "vitest",
