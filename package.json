    "terser": "^5.18.1",
    "typescript": "^4.9.5",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-solid": "^2.10.2",
