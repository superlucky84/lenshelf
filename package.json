{
  "type": "module",
  "private": true,
  "name": "state-ref-root",
  "version": "0.0.1",
  "description": "X UI Library Universal State Management Library",
  "packageManager": "pnpm@7.6.0",
  "scripts": {
    "dev:core": "pnpm --filter state-ref dev",
    "build:core": "pnpm --filter state-ref build",
    "watch:core": "vite build --watch",
    "test:core": "pnpm --filter state-ref test",
    "test:all:core": "pnpm --filter state-ref test:all",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "pnpm -r exec rm -rf dist && rm -rf dist",
    "clean:module": "pnpm -r exec rm -rf dist node_modules && rm -rf node_modules",
    "dev:preact": "pnpm watch:core & pnpm --filter @state-ref/preact dev"
  },
  "author": "superlucky84",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/lenshelf.git"
  },
  "homepage": "https://github.com/superlucky84/lenshelf",
  "license": "MIT",
  "keywords": [
    "state-ref"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/jest-dom": "^6.5.0",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/compiler-sfc": "^3.5.10",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^9.20.3",
    "jsdom": "^22.1.0",
    "lithent": "^1.13.1",
    "prettier": "^2.8.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "solid-js": "^1.9.1",
    "svelte": "^4.2.19",
    "terser": "^5.18.1",
    "typescript": "^4.9.5",
    "vite": "^5.4.8",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^2.1.2",
    "vue": "^3.5.10"
  },
  "volta": {
    "node": "20.3.0"
  }
}
