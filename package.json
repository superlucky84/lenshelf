{
  "type": "module",
  "name": "stateshelf",
  "version": "0.0.1",
  "description": "X UI Library Universal State Management Library",
  "packageManager": "pnpm@7.6.0",
  "main": "./dist/stateShelf.umd.js",
  "module": "./dist/stateShelf.mjs",
  "scripts": {
    "dev:core": "vite",
    "dev:preact": "pnpm watch:core & pnpm --filter stateshelf-preact dev",
    "build:core": "vite build",
    "build:preact": "pnpm --filter stateshelf-preact build",
    "build:!preact": "pnpm --filter '!stateshelf-preact' build",
    "watch:core": "vite build --watch",
    "watch:preact": "pnpm --filter stateshelf-preact watch",
    "build:sequence": "pnpm build:core && pnpm build:preact",
    "build:parallel": "pnpm build:!preact",
    "build": "pnpm run build:sequence && pnpm run build:parallel",
    "test:core": "vitest run --reporter verbose",
    "test": "pnpm test:core && pnpm --filter stateshelf-preact test",
    "test:dev": "vitest",
    "coverage": "vitest run --coverage",
    "clean": "pnpm clean:dist && pnpm clean:module",
    "clean:dist": "pnpm -r exec rm -rf dist && rm -rf dist",
    "clean:module": "pnpm -r exec rm -rf dist node_modules && rm -rf node_modules"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/stateshelf.mjs",
      "require": "./dist/stateshelf.umd.js"
    },
    "./preact": {
      "types": "./preact/dist/index.d.ts",
      "import": "./preact/dist/stateshelfPreact.mjs",
      "require": "./preact/dist/stateshelfPreact.umd.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "src",
    "dist",
    "preact/dist",
    "preact/src",
    "preact/package.json"
  ],
  "author": "superlucky84",
  "repository": {
    "type": "git",
    "url": "https://github.com/superlucky84/stateShelf.git"
    
  },
  "homepage": "https://github.com/superlucky84/stateShelf",
  "license": "MIT",
  "keywords": [
    "stateshelf"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "happy-dom": "^9.20.3",
    "jsdom": "^22.1.0",
    "prettier": "^2.8.8",
    "terser": "^5.18.1",
    "typescript": "^4.9.5",
    "vite": "^5.2.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^1.6.0"
  },
  "volta": {
    "node": "20.3.0"
  }
}
