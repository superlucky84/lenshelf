  },
  "homepage": "https://github.com/superlucky84/lenshelf",
  "license": "MIT",
  "keywords": [
    "lenshelf"
  ],
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@testing-library/jest-dom": "^6.5.0",
